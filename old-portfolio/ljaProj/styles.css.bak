body {
	position:relative;
}

.navbar, .navbar-brand {
	padding:30px;
	font-size:2rem;
}

#carouselHome {
	top:0;
	left:0;
	width:100%;
	height:100%;
}
.carousel-item {
	background-size: cover;
	background-position: center center;
	background-repeat: no-repeat;
	width:100%;
	height:100%;
}

.page {
	padding: 50px 0;
}
.page h2{
	text-align:center;
	margin-bottom:30px;
}