html {
	scroll-behavior: smooth;
	overflow-x: hidden;
	}
body {
	font-family: 'Cabin', sans-serif;
	color: #000;
	margin:0;
	padding:0;
	width: 100vw;

    text-align: left;
	background-image: url(https://gradywetherbee.github.io/images/beachSmall.jpg);
    background-attachment: fixed;
    background-size: cover;
    background-position: center center;
	}
#notice{
	vertical-align:center; 
	background-color:yellow; 
	z-index:100; 
	padding:30px 0;
	top:15px;
}
	
#header {
	z-index:-10;
	position:absolute;
	top:0;
	left:0;
	width:100%;
	height:4rem;
	opacity:0.87;
	background-color:#000;
	}
#logo {
	position:absolute;
	left:50%;
	top:3vh;
	margin:3rem;
	width:10vw;
}
.menuContainer {
	z-index:1;
	position:fixed;
	width:100%;
	text-align:center;
	padding-top:2rem;
	margin:-1.4vw 0;
}
.menu {
	margin:1.1vw;
	color:white;
	text-decoration:none;
	font-size:1.4vw;
}
.menu:hover {
	color:yellow;
}
#separator {
	margin:-0.3vw;
	font-size:2.0vw;
}
#separator:hover {
	color:white;
}
.arrow {
	font-family:sans-serif;
	color:#fff;
    position: absolute;
    width: 50px;
    height: 50px;
    top: 95%;
    left: 50%;
    text-align: center;
    margin: -25px 0px 0px -25px;
	font-size: 2rem;
	}
.arrow:hover {
	cursor:pointer;
    color:yellow;
	}
#contact {
